{
  "name": "adlace-builder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "i18next": "^21.9.1",
    "loglevel": "^1.8.0",
    "luxon": "^3.0.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean-dist": "rimraf ./dist/*",
    "serve-dist": "http-server ./dist -p 3001",
    "serve-hostsite": "http-server ./../hostsite -p 3002",
    "webpack-prod": "npm run clean-dist && npx cross-env NODE_ENV=production BABEL_ENV=production node ./node_modules/webpack/bin/webpack.js --config webpack.config.prod.js",
    "webpack-dev": "npm run clean-dist && npx cross-env NODE_ENV=development BABEL_ENV=development node ./node_modules/webpack/bin/webpack.js --config webpack.config.prod.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "svg-inline-loader": "^0.8.2",
    "webpack-cli": "^4.10.0"
  }
}
